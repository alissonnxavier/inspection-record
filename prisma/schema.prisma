generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  password String
  press    String[] @db.ObjectId
}

model Press {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  inspector String
  item      String
  version   String
  odf       String
  amount    String
  qtd       String
  result    String
  createdAt DateTime @default(now()) @db.Date
}

model Punching {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  item      String
  version   String
  thickness String
  odf       String
  amount    String
  qtd       String
  result    String
  createdAt DateTime @default(now()) @db.Date
  cnc       String
  inspector String
}

model Threader {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  item      String
  version   String
  odf       String
  amount    String
  qtd       String
  result    String
  createdAt DateTime @default(now()) @db.Date
  process   String
  inspector String
}

model Soldier {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  item      String
  version   String
  odf       String
  amount    String
  qtd       String
  result    String
  createdAt DateTime @default(now()) @db.Date
  process   String
  inspector String
}

model Fold {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  item      String
  version   String
  odf       String
  amount    String
  qtd       String
  result    String
  inspector String
  createdAt DateTime @default(now()) @db.Date
}

model Finishing {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  item      String
  version   String
  odf       String
  amount    String
  qtd       String
  result    String
  inspector String
  createdAt DateTime @default(now()) @db.Date
}
